    "# Sentiment Analysis:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 168,
   "id": "8bd4c940-af3b-4b68-a243-6eb8fd37b15d",
   "metadata": {},
   "outputs": [],
   "source": [
    "def compound_sentiment_columns(text):\n",
    "    regex = re.compile(\"[^a-zA-Z ]\")\n",
    "    re_clean = regex.sub('', text)\n",
    "    sentiment = analyzer.polarity_scores(re_clean)\n",
    "    compound = sentiment[\"compound\"]\n",
    "    return compound"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 170,
   "id": "9cd85ff3-586c-42dc-a544-bc1bdcd7b355",
